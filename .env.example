# Application
PORT="3000"
NODE_ENV="development"
ENABLE_TEST_USER_HEADER="true"
USE_MATERIALIZED_ELIGIBILITY="true"
ENABLE_QUERY_CACHE="false"
ENABLE_BACKGROUND_JOBS="true"
# Logging
LOG_LEVEL="debug"
